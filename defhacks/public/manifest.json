{
 "manifest_version": 2,
 "name": "LendMe",
 "description": "Even Financial Chrome Extension",
 "version": "1",
 "author": "Indos",
 "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Open new window"
  },
 "content_scripts": [
   {
     "matches": [
        "https://www.amazon.com/gp/buy/spc/handlers/display.html?hasWorkingJavascript=1"
     ],
     "js": ["openNewWindow.js"]
   }
 ],
 "permissions": [ "activeTab"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}