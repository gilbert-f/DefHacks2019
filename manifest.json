{
 "manifest_version": 2,
 "name": "DefHacks",
 "description": "Plugin",
 "version": "1",
 "author": "Indos",
 "browser_action": {
    "default_icon": "icon.png",
   "default_title": "Play",
   "default_popup": "result-table/index.html"
  },
 "content_scripts": [
   {
     "matches": [
        "https://www.amazon.com/gp/buy/spc/handlers/display.html?hasWorkingJavascript=1"
     ],
     "js": ["content.js"]
   }
 ],
 "permissions": ["activeTab"
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}